<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schedule.ReportController">
    <children>
        <Label text="Reports"/>
        <Button text="Generate Reports" onAction="#generateReports"/>
        <TextArea fx:id="reportArea" editable="false" prefHeight="400.0" prefWidth="600.0"/>
        <Button text="Back to Home" onAction="#backToHome"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
